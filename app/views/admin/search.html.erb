<h3>Search Results:</h3>
<ul class="no-bullets">
  <% @students.each do |student| %>
    <li> <%= link_to "#{student.full_name}", student_path(student) %>
      <%= link_to "<i class='fa fa-users' aria-hidden='true'></i>".html_safe, summary_admin_index_path(family_id: student.family.id), id: "family-icon" %>
    </li>
  <% end %>
</ul>
